# ClearCollect AI — Environment Variables
# Copy this file to .env and fill in your values.

# CORS origins (comma-separated)
CORS_ORIGINS=http://localhost:5173

# Database URL
DATABASE_URL=sqlite:///./clearcollect.db

# Upload directory
UPLOAD_DIR=./uploads
MAX_UPLOAD_SIZE_MB=50

# Gemini API key (optional — for executive summaries)
GEMINI_API_KEY=
